{
    "version": 2,
    "builds": [
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "index.js"
      }
    ],

    "functions": {
      "src/files/": {
        "memory": 1024,
        "maxDuration": 10
      }
    }
  }